    <handleGitHub>true</handleGitHub>
    <handleLudeon>true</handleLudeon>
    <handleSteam>true</handleSteam>
    <handleDiscord>true</handleDiscord>
    <handleDiscordWebhook>true</handleDiscordWebhook>
    <handlePatreon>true</handlePatreon>
    <showCopyableNotes>true</showCopyableNotes>
    <copyableNotesProgram>notepad.exe</copyableNotesProgram>
    <filteredWhenCopied>.git, \\source, .gitattributes, .gitignore, .idea, .vs, .exe, Octokit</filteredWhenCopied>
    <filteredWhenZipped>PublishedFileId, Deployer, RimworldModReleaseTool, updateinfo, Octokit</filteredWhenZipped>
    <githubProductHeaderValue>RimworldModReleaseTool</githubProductHeaderValue>
    <githubCommitter>Rimworld Mod Release Tool</githubCommitter>
    <githubCommitterEmail>anonymous@gmail.com</githubCommitterEmail>
